{
  "extends": ["eslint-config-with-prettier"],
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/no-dynamic-require": 0,
    "import/extensions": 0,
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0,
    "no-underscore-dangle": 0,
    "no-restricted-globals": 0,
    "no-plusplus": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "none",
        "bracketSpacing": true,
        "semi": false,
        "singleQuote": true
      }
    ]
  }
}

